{
  "installCommand": "cd frontend && npm ci --include=dev",
  "buildCommand": "cd frontend && npm ci --include=dev && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
